		"chai": "^4.3.4",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"got": "^11.8.2",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte-preprocess": "^4.9.8",
		"typescript": "^4.4.4",
		"vite": "^2.6.13"
	},
	"dependencies": {
		"@sveltejs/kit": "1.0.0-next.192",
		"date-fns": "^2.25.0",
